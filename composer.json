{
	"name" : "hofff/contao-htaccess",
	"description" : "Backend integrated htaccess editor for Contao Open Source CMS",
	"keywords" : [
		"contao",
		"htaccess"
	],
	"type" : "contao-module",
	"homepage" : "https://github.com/hofff/contao-htaccess",
	"license" : "LGPL-3.0+",
	"authors" : [{
			"name" : "Oliver Hoff",
			"email" : "oliver@hofff.com",
			"homepage" : "https://hofff.com/",
			"role" : "Developer"
		},
		{
			"name": "Cliff Parnitzky",
			"homepage": "https://hofff.com",
			"email": "cliff@hofff.com",
			"role": "Developer"
		},
		{
			"name": "Nicky Hoff",
			"homepage": "https://hofff.com",
			"email": "nick@hofff.com",
			"role": "Manager"
		}
	],
	"support" : {
		"email" : "info@hofff.com",
		"issues" : "https://github.com/hofff/contao-htaccess/issues",
		"wiki" : "https://github.com/hofff/contao-htaccess/wiki",
		"irc" : "irc://irc.freenode.org/contao.dev",
		"source" : "https://github.com/hofff/contao-htaccess"
	},
	"require" : {
		"php" : ">=5.4",
		"contao-community-alliance/composer-plugin": "~2.0",
		"contao/core" : "~3.2",
		"contao-community-alliance/dc-general" : "~2.0-beta13",
		"contao-community-alliance/dc-general-data-provider-php-globals-config-file" : "~1.0",
		"contao-community-alliance/event-dispatcher" : "~1.0",
		"bit3/contao-meta-palettes" : "~1.6",
		"bit3/string-builder" : "~2.0",
		"menatwork/contao-multicolumnwizard" : "~3.2"
	},
	"suggest" : {
		"psi/redirect4ward" : "Define page redirection rules."
	},
	"replace" : {
		"contao-legacy/htaccess" : "self.version",
		"bit3/contao-htaccess" : "self.version"
	},
	"autoload" : {
		"psr-4" : {
			"Bit3\\Contao\\Htaccess\\" : "src"
		}
	},
	"extra" : {
		"contao" : {
			"sources" : {
				"assets" : "assets/htaccess",
				"module" : "system/modules/htaccess"
			}
		},
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}